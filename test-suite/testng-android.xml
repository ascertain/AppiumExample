<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Appium Sample tests for Android">
    <test name="Android Tests on Mobile Browsers" enabled="true">
        <classes>
            <class name="io.github.mdk.android.tests.AndroidTests">
                <methods>
                    <exclude name="testChrome"/>
                    <include name="testSuccessfulSearch"/>
                    <include name="testNoResultsForInvalidSearch"/>
                    <include name="testSearchResultDisplayFeature"/>
                </methods>
            </class>
        </classes>
    </test>
</suite> <!-- Suite -->